<%= render 'shared/page_title', title: 'All Users' %>

<ul class ="nav nav-tabs">
    <li>
        <%= link_to 'Contracts', contracts_path %>
    </li>
    <li class="active">
        <%= link_to 'Users', users_path %>
    </li>
    
</ul>

<div class ="row">
    <div class="col-md-4 col-md-offset-4 center">
        <%= will_paginate %>
        
    </div>
</div>

<% @users.each do |user| %>
    <div class ="row">
        <div class="col-md-4 col-md-offset-4 center">
          <%= link_to gravatar_for(user, size: 200), user_path(user) %>
          <p>
              <h5>User Name: <%= user.username %> </h5>
          </p>
    </div>
</div>

<% end %>